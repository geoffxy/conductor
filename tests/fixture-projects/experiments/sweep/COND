run_experiment_group(
  name="threads",
  run="./run.sh",
  experiments=[
    ExperimentInstance(
      name="threads-{}".format(threads),
      options={"threads": threads},
    )
    for threads in range(1, 5)
  ],
)

run_experiment_group(
  name="threads-args",
  run="./run.sh",
  experiments=[
    ExperimentInstance(
      name="threads-args-{}".format(threads),
      args=[threads],
    )
    for threads in range(1, 5)
  ],
)
